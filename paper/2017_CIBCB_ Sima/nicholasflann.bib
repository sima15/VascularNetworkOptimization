    pages = {171--187},
    posted-at = {2014-06-04 22:34:09},
    priority = {2},
    publisher = {SAGE Publications},
    title = {{Semi-automatic drawing of metabolic networks}},
    url = {http://dx.doi.org/10.1177/1473871611413565},
    volume = {11},
