    pages = {95--117},
    pmid = {12678905},
    posted-at = {2008-04-05 18:25:41},
    priority = {4},
    title = {{Vascular endothelial cell growth factor (VEGF), an emerging target for cancer chemotherapy.}},
    url = {http://view.ncbi.nlm.nih.gov/pubmed/12678905},
    volume = {3},
    year = {2003}
}

