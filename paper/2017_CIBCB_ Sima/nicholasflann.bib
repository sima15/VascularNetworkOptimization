    posted-at = {2011-12-30 02:35:45},
    priority = {2},
    publisher = {Public Library of Science},
    title = {{Adaptation of Saccharomyces cerevisiae Cells to High Ethanol Concentration and Changes in Fatty Acid Composition of Membrane and Cell Size}},
