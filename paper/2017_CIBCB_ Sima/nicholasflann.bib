    journal = {Biophysical journal},
    keywords = {filopodia},
    month = may,
    number = {10},
    pages = {3839--3852},
    pmcid = {PMC2367176},
    pmid = {18234810},
    posted-at = {2008-12-22 00:41:59},
    priority = {3},
