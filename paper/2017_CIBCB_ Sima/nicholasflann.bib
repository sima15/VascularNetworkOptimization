    pages = {545--555},
    posted-at = {2009-01-16 20:33:28},
    priority = {4},
    title = {{Vascular sprout formation entails tissue deformations and VE-cadherin-dependent cell-autonomous motility}},
    url = {http://dx.doi.org/10.1016/j.ydbio.2007.10.036},
    volume = {313},
