    issn = {00225193},
    journal = {Journal of Theoretical Biology},
    keywords = {filopodia},
    month = aug,
    number = {4},
    pages = {701--716},
    posted-at = {2008-12-22 00:20:23},
    priority = {4},
    title = {{A computational model of cell migration coupling the growth of focal adhesions with oscillatory cell protrusions}},
    url = {http://dx.doi.org/10.1016/j.jtbi.2008.04.035},
    volume = {253},
